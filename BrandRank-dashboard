        // Helper functions
        function escapeForRegex(str) {
            return str.replace(/[.*+?^${}()|[\]\\]/g, '\\$&');
        }

        function restoreOriginalCells() {
            const cells = document.querySelectorAll('[data-original-text]');
            cells.forEach(cell => {
                cell.innerHTML = cell.getAttribute('data-original-text');
            });
        }

        function canonicalizeBrand(brandName) {
            const aliases = window.WINDOW_DATA.brandAliases;
            for (let canonical in aliases) {
                if (canonical.toLowerCase() === brandName.toLowerCase()) {
                    return canonical;
                }
                if (aliases[canonical].some(alias => alias.toLowerCase() === brandName.toLowerCase())) {
                    return canonical;
                }
            }
            return brandName;
        }

        function getAllBrandVariants(canonical) {
            const aliases = window.WINDOW_DATA.brandAliases;
            const variants = [canonical];
            if (aliases[canonical]) {
                variants.push(...aliases[canonical]);
            }
            return variants;
        }

        function computeBrandCounts() {
            const brandCounts = {};
            const cells = document.querySelectorAll('.llm-response, .prompt-cell');
            
            cells.forEach(cell => {
                const text = cell.textContent;
                const words = text.match(/[\w\.]+(?:\s+[\w\.]+)*/g) || [];
                
                words.forEach(word => {
                    const canonical = canonicalizeBrand(word);
                    if (canonical && canonical.length > 2) {
                        brandCounts[canonical] = (brandCounts[canonical] || 0) + 1;
                    }
                });
            });

            // Sort by count descending, then alphabetically
            const sorted = Object.entries(brandCounts)
                .sort((a, b) => {
                    if (b[1] !== a[1]) return b[1] - a[1];
                    return a[0].localeCompare(b[0]);
                })
                .slice(0, 10);

            return sorted;
        }

        let activeHighlights = new Set();

        function highlightMatches(canonicalBrands, scrollToFirst = false) {
            restoreOriginalCells();
            
            const allTerms = [];
            canonicalBrands.forEach(canonical => {
                allTerms.push(...getAllBrandVariants(canonical));
            });

            const searchTerm = document.getElementById('searchInput').value.trim();
            if (searchTerm) {
                allTerms.push(searchTerm);
            }

            if (allTerms.length === 0) {
                document.getElementById('searchResults').textContent = '';
                return;
            }

            const cells = document.querySelectorAll('[data-original-text]');
            let totalMatches = 0;
            let firstMatch = null;

            cells.forEach(cell => {
                let html = cell.getAttribute('data-original-text');
                
                allTerms.forEach(term => {
                    const escapedTerm = escapeForRegex(term);
                    const regex = new RegExp(`\\b(${escapedTerm})\\b`, 'gi');
                    const matches = html.match(regex);
                    if (matches) {
                        totalMatches += matches.length;
                        if (!firstMatch) firstMatch = cell;
                    }
                    html = html.replace(regex, '<span class="highlight">$1</span>');
                });

                cell.innerHTML = html;
            });

            document.getElementById('searchResults').textContent = 
                totalMatches > 0 ? `${totalMatches} matches found` : 'No matches found';

            if (scrollToFirst && firstMatch) {
                firstMatch.scrollIntoView({ behavior: 'smooth', block: 'center' });
            }
        }

        function buildBrandBar() {
            const topBrands = computeBrandCounts();
            const brandBar = document.getElementById('brandBar');
            
            if (topBrands.length === 0) {
                brandBar.style.display = 'none';
                return;
            }

            let html = '<h3>Top Brands by Mentions</h3><div class="brand-buttons">';
            
            topBrands.forEach(([brand, count]) => {
                html += `<button class="brand-btn" data-brand="${brand}">${brand} (${count})</button>`;
            });
            
            html += '<button class="brand-clear" id="clearAllBtn">Clear All</button>';
            html += '</div>';
            
            brandBar.innerHTML = html;

            // Wire up click handlers
            document.querySelectorAll('.brand-btn').forEach(btn => {
                btn.addEventListener('click', function() {
                    const brand = this.getAttribute('data-brand');
                    
                    if (activeHighlights.has(brand)) {
                        activeHighlights.delete(brand);
                        this.classList.remove('brand-btn-active');
                    } else {
                        activeHighlights.add(brand);
                        this.classList.add('brand-btn-active');
                    }
                    
                    highlightMatches(Array.from(activeHighlights), true);
                });
            });

            document.getElementById('clearAllBtn').addEventListener('click', function() {
                activeHighlights.clear();
                document.querySelectorAll('.brand-btn').forEach(btn => {
                    btn.classList.remove('brand-btn-active');
                });
                highlightMatches([]);
            });
        }

        function renderCategories() {
            const container = document.getElementById('categoriesContainer');
            const data = window.WINDOW_DATA;
            
            let html = '';
            
            data.categories.forEach(category => {
                html += `
                    <div class="category-section">
                        <div class="category-header">
                            <h2>${category.name}</h2>
                            <p class="category-stats">${category.prompts.length} prompts analyzed across ${data.llms.length} AI models</p>
                        </div>
                        <div class="table-container">
                            <table class="data-table">
                                <thead>
                                    <tr>
                                        <th>Prompt Question</th>
                                        ${data.llms.map(llm => `<th>${llm}</th>`).join('')}
                                    </tr>
                                </thead>
                                <tbody>
                                    ${category.prompts.map(prompt => `
                                        <tr>
                                            <td class="prompt-cell" data-original-text="${prompt.question}">${prompt.question}</td>
                                            ${data.llms.map(llm => 
                                                `<td class="llm-response" data-original-text="${prompt.responses[llm] || 'N/A'}">${prompt.responses[llm] || 'N/A'}</td>`
                                            ).join('')}
                                        </tr>
                                    `).join('')}
                                </tbody>
                            </table>
                        </div>
                    </div>
                `;
            });
            
            container.innerHTML = html;
        }

        function initSearch() {
            const searchInput = document.getElementById('searchInput');
            const clearBtn = document.getElementById('clearSearchBtn');
            
            searchInput.addEventListener('input', function() {
                highlightMatches(Array.from(activeHighlights));
            });

            clearBtn.addEventListener('click', function() {
                searchInput.value = '';
                highlightMatches(Array.from(activeHighlights));
            });
        }

        function init() {
            renderCategories();
            buildBrandBar();
            initSearch();
        }

        // Initialize on load
        document.addEventListener('DOMContentLoaded', init);
    </script>
</body>
</html>
